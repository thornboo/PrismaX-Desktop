{
  "name": "prismax-desktop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron-dist/main.cjs",
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "better-sqlite3",
      "esbuild"
    ]
  },
  "electronmon": {
    "patterns": [
      "electron-dist/**/*"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently -k \"vite\" \"pnpm build:electron --watch\" \"wait-on tcp:3000 && electronmon electron-dist/main.cjs\"",
    "build": "vite build",
    "build:electron": "tsup",
    "build:all": "pnpm build && pnpm build:electron",
    "rebuild:native": "electron-rebuild -f -w better-sqlite3",
    "package": "pnpm build:all && electron-builder",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.1.0",
    "@lancedb/lancedb": "0.22.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "ai": "^4.1.0",
    "apache-arrow": "18.1.0",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.3",
    "framer-motion": "^11.15.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.469.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.1.1",
    "rehype-highlight": "^7.0.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.2"
  },
  "optionalDependencies": {
    "@lancedb/lancedb-darwin-arm64": "0.22.3",
    "@lancedb/lancedb-darwin-x64": "0.22.3",
    "@lancedb/lancedb-linux-arm64-gnu": "0.22.3",
    "@lancedb/lancedb-linux-arm64-musl": "0.22.3",
    "@lancedb/lancedb-linux-x64-gnu": "0.22.3",
    "@lancedb/lancedb-linux-x64-musl": "0.22.3",
    "@lancedb/lancedb-win32-arm64-msvc": "0.22.3",
    "@lancedb/lancedb-win32-x64-msvc": "0.22.3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@eslint/js": "^9.20.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.30.1",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "electronmon": "^2.0.4",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.15.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.0.7",
    "wait-on": "^8.0.2"
  }
}
